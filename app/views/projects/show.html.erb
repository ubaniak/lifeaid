<div class="content">
    <p id="notice"><%= notice %></p>
    <div class="" style="float:left;">
        <div id="circle" class="<%= @project.status.name %>"></div>
    </div>
    <h3> <%= @project.title %></h3>
    <div class="row">
        <div class="span5"></div>
        <div class="span7">
            <%= simple_format @project.content %>
          <span class="italics">
            Created by:
            <%= @project.user.name %>
          </span>
        </div>
    </div>
</div>
